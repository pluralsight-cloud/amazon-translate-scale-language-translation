{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Principal": {
          "Service": "lambda.amazonaws.com"
        },
        "Action": "sts:AssumeRole",
        "Condition": {
          "ArnLike": {
              "aws:SourceArn": "arn:aws:lambda:*:804540873837:*"
          },
          "StringEquals": {
              "aws:SourceAccount": "804540873837"
          }
        }
      }
    ]
  }